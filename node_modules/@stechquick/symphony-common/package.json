{
    "name": "@stechquick/symphony-common",
    "version": "0.1.36",
    "description": "symphony common library",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
        "version:inc": "node ./lib/executables/version.js",
        "go": "npm run link:self && npm run build",
        "go:watch": "npm run link:self && npm run build:clean:watch",
        "link:self": "npm link",
        "build": "tsc",
        "build:clean": "npm run remove:lib && npm run build",
        "build:watch": "npm run build -- --watch",
        "build:clean:watch": "npm run remove:lib && npm run build:watch",
        "remove:lib": "rm -rf ./lib/*",
        "pack": "npm pack",
        "publish:do": "npm publish --access public"
    },
    "dependencies": {
        "@stechquick/algae": "^1.0.356"
    },
    "devDependencies": {
        "@types/node": "^14.10.1",
        "typescript": "4.4.2"
    },
    "files": [
        "lib"
    ],
    "author": "",
    "license": "ISC"
}