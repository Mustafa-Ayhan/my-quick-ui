{
    "name": "@stechquick/symphony-nanoserver-wsplugin",
    "version": "0.2.8",
    "description": "quick symphony nano server",
    "main": "./dist/nanoSocketServer.js",
    "scripts": {
        "version:inc": "node node_modules/@stechquick/symphony-common/lib/executables/version",
        "go": "npm run linkSelf && npm run link && npm run build",
        "goWatch": "npm run linkSelf && npm run link && npm run buildWatch",
        "linkSelf": "npm link",
        "link": "npm link @stechquick/symphony-common @stechquick/symphony-nanoserver",
        "build": "npm run removeDist && tsc",
        "buildWatch": "npm run removeDist && tsc --watch",
        "removeDist": "rm -rf dist/*",
        "pack": "npm pack"
    },
    "devDependencies": {
        "@types/node": "^12.19.1",
        "@stechquick/symphony-common": "^0.1.34",
        "typescript": "^3.7.3",
        "@stechquick/algae": "1.0.356"
    },
    "files": [
        "dist"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/ws": "^7.4.7",
        "@stechquick/symphony-nanoserver": "^0.2.8",
        "ws": "^7.5.10"
    },
    "security-updates": {
        "package": "ws",
        "new_version": "7.5.10",
        "reason": "https://github.com/advisories/GHSA-3h5v-q93c-6h6q"
    }
}