{
    "name": "@stechquick/algae",
    "version": "1.0.405",
    "description": "STechQuick Common Library",
    "main": "index.js",
    "scripts": {
        "build": "npm run remove:lib && npm run version:file && tsc && npm run copyLibFiles",
        "build:clean": "npm run remove:lib && npm run build",
        "build:watch": "npm run build -- --watch",
        "build:clean:watch": "npm run remove:lib && npm run build:watch",
        "clean": "npm run remove:lib",
        "version:file": "node ./src/executables/versionInfoFile",
        "remove:lib": "rm -rf ./lib/*",
        "go": "npm run link:self && npm run build",
        "go:watch": "npm run link:self && npm run build:clean:watch",
        "version:inc": "node ./lib/executables/version.js",
        "pack": "npm pack",
        "copyLibFiles": "node ./executables/copyLibFiles"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/STechQ/algae.git"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/archiver": "^5.3.2",
        "@types/node": "^14.10.1",
        "@types/qrcode": "^1.5.0",
        "chalk": "^4.1.0",
        "qrcode": "^1.5.3",
        "typescript": "4.4.2"
    },
    "files": [
        "lib"
    ],
    "dependencies": {
        "fs": "^0.0.1-security",
        "path": "^0.12.7"
    }
}