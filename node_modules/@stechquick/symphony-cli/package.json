{
    "name": "@stechquick/symphony-cli",
    "description": "quick symphony server cli tools",
    "version": "0.5.33",
    "bin": {
        "symphony-cli": "./bin/run"
    },
    "bugs": "https://github.com/alper-batioglu/symphony-cli/issues",
    "scripts": {
        "link": "npm link @stechquick/symphony-common @stechquick/symphony-nanoserver @stechquick/symphony-nanoserver-wsplugin",
        "postpack": "rimraf oclif.manifest.json",
        "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif-dev readme && git add README.md",
        "build": "npm run build -w symtypeconv && tsc -b",
        "build:watch": "tsc -b --watch",
        "forceInstall": "npm install @stechquick/symphony-common --save --force && npm install @stechquick/symphony-nanoserver --save --force",
        "version:inc": "node node_modules/@stechquick/symphony-common/lib/executables/version",
        "pack": "npm pack"
    },
    "dependencies": {
        "@oclif/command": "^1.6.1",
        "@oclif/config": "^1.15.1",
        "@oclif/plugin-help": "^3.1.0",
        "@stechquick/algae": "^1.0.356",
        "@stechquick/symphony-common": "^0.1.34",
        "@stechquick/symphony-nanoserver": "^0.2.18",
        "@stechquick/symphony-nanoserver-wsplugin": "^0.2.8",
        "@types/js-yaml": "^3.12.5",
        "cli-ux": "^5.4.6",
        "debug": "^4.1.1",
        "fs-extra": "^9.0.1",
        "js-yaml": "^3.14.0",
        "tslib": "^1.13.0",
        "typeconv": "^2.3.1"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.22.2",
        "@oclif/test": "^1.2.6",
        "@types/chai": "^4.2.11",
        "@types/mocha": "^5.2.7",
        "@types/node": "^10.17.26",
        "chai": "^4.2.0",
        "globby": "^10.0.2",
        "mocha": "^5.2.0",
        "nyc": "^14.1.1",
        "rimraf": "^3.0.2",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.5"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json",
        "/templates",
        "symtypeconv/dist"
    ],
    "homepage": "https://github.com/alper-batioglu/symphony-cli",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "symphony-cli",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": "alper-batioglu/symphony-cli",
    "types": "lib/index.d.ts",
    "workspaces": [
        "symtypeconv"
    ]
}