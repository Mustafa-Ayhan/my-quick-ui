{
  "name": "@stechquick/qui",
  "version": "3.1.4-3-BETA-release-3.1.4-4-2025.11.24.13.36",
  "description": "Quick UI",
  "main": "index.js",
  "scripts": {
    "//// ---------------   BUILDS   --------------- \\\\": "----------------------------------------------",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:prod": "npm run clean && npm run build:server:prod && npm run build:client:prod",
    "build:client": "node --max-old-space-size=8192 node_modules/webpack/bin/webpack.js --config ./webpack.config.js",
    "build:client:prod": "node --max-old-space-size=8192 node_modules/webpack/bin/webpack.js --config ./webpack.prod.config.js",
    "build:server": "node --max-old-space-size=8192 node_modules/webpack/bin/webpack.js --context ./server --config ./server/webpack.config.js",
    "build:server:prod": "node --max-old-space-size=8192 node_modules/webpack/bin/webpack.js --context ./server --config ./server/webpack.config.prod.js",
    "//// ------------   BUILD WATCHES   ----------- \\\\": "----------------------------------------------",
    "build:watch": "npm run clean && npm run build:server && npm run build:client -- --watch",
    "build:server:watch": "npm run build:server -- --watch",
    "//// -------------    CLEAN UPS   ------------- \\\\": "----------------------------------------------",
    "clean": "rm -rf dist/*",
    "//// ---------------   OTHERS   --------------- \\\\": "----------------------------------------------",
    "test": "jest --colors --coverage",
    "link": "npm link @stechquick/symphony-nanoserver",
    "//// ---------------   STARTS   --------------- \\\\": "----------------------------------------------",
    "start": "node ./dist/server/server.js -p 3001 -r ./dist/client -env local"
  },
  "devDependencies": {
    "@stechquick/symphony-nanoserver": "^0.2.7",
    "@types/args": "^5.0.0",
    "@types/jest": "^27.0.2",
    "@types/js-yaml": "^4.0.3",
    "@types/node": "^16.10.3",
    "@types/yargs": "^17.0.3",
    "@vue/compiler-sfc": "^3.2.26",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "6.7.2",
    "fastify": "3.27.1",
    "fastify-cors": "^6.0.2",
    "fastify-http-proxy": "^6.2.1",
    "fastify-reply-from": "6.4.1",
    "fastify-static": "~4.4.0",
    "jest": "^27.2.4",
    "js-yaml": "^4.1.0",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "source-map-loader": "4.0.1",
    "style-loader": "^3.3.0",
    "stylus": "^0.55.0",
    "stylus-loader": "^6.1.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "undici": "4.12.2",
    "vue": "^2.6.14",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.10.1",
    "vue-template-compiler": "^2.6.14",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-merge": "^5.8.0",
    "webpack-utf8-bom": "^1.3.0",
    "webpackbar": "5.0.2",
    "wrapper-webpack-plugin": "^2.1.0"
  },
  "files": [
    "dist"
  ]
}